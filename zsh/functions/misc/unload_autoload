## vim:ft=zsh ts=2 sw=2 sts=2 et fenc=utf-8
################################################
# * This function unfunction an autoloaded function
# * and autoloads it again 
# *
################################################

emulate -L zsh
setopt extendedglob localoptions

if (( ${+argv[1]} )); then
  unfunction "$argv[@]"
  autoload -Uz "$argv[@]"
fi
